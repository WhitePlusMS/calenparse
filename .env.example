# 访客模式配置
VITE_VISITOR_LLM_QUOTA=3     # 每日 LLM 调用次数限制
VITE_VISITOR_EVENT_QUOTA=5   # 每日/总共 事件创建数量限制 (具体逻辑由后端 RLS 和前端逻辑共同控制)

# Supabase Configuration
VITE_SUPABASE_URL=https://otythkidkgvmpazemidt.supabase.co
VITE_SUPABASE_ANON_KEY=

# LLM API Configuration
# Provider: 'zhipu' (默认, 智谱AI标准格式) 或 'baidu' (百度文心)
VITE_LLM_PROVIDER=zhipu

# 智谱 AI (BigModel) 配置示例
VITE_LLM_API_KEY=your_zhipu_api_key
VITE_LLM_API_ENDPOINT=https://open.bigmodel.cn/api/paas/v4/chat/completions
VITE_LLM_MODEL=glm-4-flash

